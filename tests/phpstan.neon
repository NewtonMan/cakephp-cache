parameters:
	autoload_files:
		- %rootDir%/../../../tests/bootstrap.php
		- %rootDir%/../../../tests/shim.php
	ignoreErrors:
		- '#Access to protected property .+\:\:.+.#'
		- '#PHPDoc tag @param for parameter \$request with type .+ServerRequestInterface is not subtype of native type .+ServerRequest.#'
		- '#PHPDoc tag @param for parameter \$response with type .+ResponseInterface is not subtype of native type .+Response.#'
		- '#Parameter \#1 \$function of function call_user_func expects callable\(\): mixed, mixed given.#'
